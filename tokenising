#include <iostream>
#include<bits/stdc++.h>
#include<string.h>

using namespace std;

int main() {
	char gfg[100] = " Geeks  for  geeks  Contribute";

	// Declaration of delimiter
	const char s[4] = " ";
	char* tok;

	// Use of strtok
	// get first token
	tok = strtok(gfg, s);

	// Checks for delimeter
	while (tok != 0) {
		printf(" %s\n", tok);

		// Use of strtok
		// go through other tokens
		tok = strtok(0, s);
	}

    return 0;
}
